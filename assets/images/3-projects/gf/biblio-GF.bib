journal = {2012 MRS Fall Meeting},
keywords = {3D printers,Assembly line,Coatings,Electronic codes,Extrusion process,Films and coatings,Glass,Manufacture,Manufacturing IS,Materials handling,Printing,Security printing,Three-dimensional printing,Traditional manufacturing},
pages = {73--77},
title = {{Glass: An old material for the future of manufacturing}},
url = {http://www.scopus.com/inward/record.url?eid=2-s2.0-84889682480{\&}partnerID=40{\&}md5=02eb830c15e289971f8d1929c1ccb1c1},
