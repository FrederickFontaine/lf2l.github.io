keywords = {Additive Manufacturing,Additive laser manufacturing,Design for sustainable additive manufacturing,Environmental sustainability,additive laser manufacturing,additive manufacturing,design for sustainable additive,environmental sustainability,manufacturing},
pages = {26--31},
publisher = {Elsevier B.V.},
title = {{Predictive Model for Environmental Assessment in Additive Manufacturing Process}},
